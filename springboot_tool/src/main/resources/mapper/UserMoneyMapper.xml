<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.xuegao.springboot_tool.dao.IUserMoneyMapper">

    <update id="decrementMoneyByUserId">
        update mq_user_money set money = money - #{money} where user_id = #{userId}
    </update>
</mapper>